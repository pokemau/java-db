<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csit228f2_2.CreateNoteController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="700.0" spacing="10.0">
         <children>
            <TextField fx:id="titleField" promptText="Enter title..." />
            <TextArea fx:id="contentField" prefHeight="314.0" prefWidth="680.0" />
            <Button mnemonicParsing="false" onAction="#onSaveClick" text="Save" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
