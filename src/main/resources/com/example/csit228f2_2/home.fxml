<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" stylesheets="@home.css" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csit228f2_2.HomeController">
   <children>
      <VBox prefHeight="400.0" prefWidth="700.0">
         <children>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="700.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onCreateNoteClick" prefWidth="70.0" text="New Note" />
                  <Separator prefHeight="0.0" prefWidth="537.0" />
                  <Button mnemonicParsing="false" onAction="#logout" prefWidth="70.0" text="Logout" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <ScrollPane prefHeight="477.0" prefWidth="700.0">
              <content>
                <AnchorPane fx:id="mainAnchorPane">
                     <children>
                        <VBox fx:id="notesContainer" />
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
